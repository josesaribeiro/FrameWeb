<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN"
        "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:h="http://xmlns.jcp.org/jsf/html">
	
	{% for form in forms %}
	    {% set dependency = form.ClientDependencies %}
    <h:form id="{{ form.Name }}">
    	{% for attribute in form.Attributes %}
    		{% if attribute.Type.Name == "inputText" %}
    	<p:inputText id="{{ attribute.Name }}" value="#{}" />
    		{% elseif attribute.Type.Name == "inputNumber" %}
    	<p:inputNumber id="{{ attribute.Name }}" value="#{}" />
    		{% elseif attribute.Type.Name == "outputLabel" %}
    	<p:outputLabel for="{{ attribute.Name }}" value="VALUE OF {{ attribute.Name }}" />
    		{% elseif attribute.Type.Name == "commandButton" %}
	    		{% set methodName = dependency.Method.Name %}
	    		{% set controllerName = (dependency.Targets[0]).Name %} 
		<h:commandButton value="{{ attribute.Name }}" action="#'{'{{ methodName }}.{{ controllerName }}'}'" />
    		{% endif %}
        {% endfor %}
    </h:form>
    {% endfor %}
</html>
